      <section class="boxscores <%= boxscore.formatted_game_date %> <%= boxscore.team.downcase %> <%= boxscore.opponent.downcase %>">
      <%= render :partial => 'aggregate_info/boxscore/section_header', :locals => {:boxscore => boxscore, :expanded => expanded } %>
        <div class="boxscore-content">
          <%= render :partial => 'aggregate_info/boxscore/boxscore', :locals => {:boxscore_lines => boxscore.team_lines, :class_label => "team-boxscore", :expanded => expanded, :selected => true } %>
          <%= render :partial => 'aggregate_info/boxscore/boxscore', :locals => {:boxscore_lines => boxscore.opponent_lines, :class_label => expanded ? "team-boxscore" : "opponent-boxscore", :expanded => expanded, :selected => false } %>
        </div>
        <div class="event-timeline-container">
          <div class="event-timeline"></div>
        </div>
      </section>
