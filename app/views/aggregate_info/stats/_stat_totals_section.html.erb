    <section class="<%= title.split.join("-").downcase %>">
      <div class="section-header clearfix">
        <h1><a href="#<%= title.split.join("-").downcase %>"><%= title %></a></h1>
        <nav class="section-content"><li class="selected" data-type="totals">Totals</li><li data-type="per-game">Per Game</li><li data-type="per-36" class="per-36">Per 36 Mins</li><li data-type="advanced">Advanced</li></nav>
        <span class='split-type'>
          <%= render :partial => 'aggregate_info/stats/splits_drop_down' %>
        </span>
      </div>
      <div class="section-content stat-totals">
        <div class="tabled-data">
          <%= render :partial => 'aggregate_info/stats/stat_totals', :locals => { :lines => lines } %>
        </div>
        <div>
          <%= render :partial => 'aggregate_info/stats/graph_container' %>
        </div>
      </div>
    </section>
