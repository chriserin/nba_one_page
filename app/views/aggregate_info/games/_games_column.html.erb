        <ul>
        <% @team_rep.games[start..finish].each do |game| %>
          <% if game.played? %>
            <%= render :partial => 'aggregate_info/games/played_game_item', :locals => {:game => game} %>
          <% else %>
            <%= render :partial => 'aggregate_info/games/unplayed_game_item', :locals => {:game => game} %>
          <% end %>
        <% end %>
        </ul>
